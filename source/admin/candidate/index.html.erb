
<!-- page heading -->
<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">
      Pré-candidato <small>Informações de Pré-candidatos</small>
    </h1>
    <ol class="breadcrumb">
      <li> <i class="fa fa-dashboard"></i> Dashboard</li>
      <li class="active"> Pré-cantidato</li>
    </ol>
  </div>
</div>
<!-- /.row -->


<div class="row" ng-controller="PreCandidateController">
    <div class="col-lg-12">
      <fieldset>
        <legend>Dados do pré-candidato - #{{model.id}}</legend>
        <form class="form-horizontal">
          <div class="form-group">
            <label class="col-sm-2 control-label">ID</label>
            <div class="col-sm-10">
              <span>{{model.id}}</span>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-2 control-label">Nome de usuário</label>
            <div class="col-sm-10">
              <div class="input-group">
                <input class="form-control" placeholder="seunome" ng-model="model.username" type="text" disabled>
                <span class="input-group-addon" id="basic-addon2">.votolegal.org.br</span>
              </div>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-2 control-label">Nome</label>
            <div class="col-sm-10">
              <input type="text" class="form-control" ng-model="model.name" disabled>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-2 control-label">E-mail Publico</label>
            <div class="col-sm-10">
              <input type="text" class="form-control" ng-model="model.public_email" disabled>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-2 control-label">Status do Cadastro</label>
            <div class="col-sm-10">
              <span class="label label-warning">{{ model.status == 'pending' ? 'Pendente' : (model.status == 'deactivated' ? 'Inativo' : (model.status == 'activated' ? 'Ativo' : '') ) }}</span>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-2 control-label">Cargo Político</label>
            <div class="col-sm-10">
              <span class="label label-default">{{ model.office_id == 1 ? 'Prefeito' : 'Vereador' }}</span>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-2 control-label">Partido</label>
            <div class="col-sm-10">
              <input type="text" class="form-control" ng-model="model.party.acronym" disabled>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-2 control-label">Reeleição</label>
            <div class="col-sm-10">
              <span class="label label-default">{{ model.reelection == 1 ? 'Sim' : 'Não' }}</span>
            </div>
          </div>
          <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
              <button type="button" class="btn btn-primary" ng-click="window.back(-1)">Voltar</button>
            </div>
          </div>
        </form>
      </fieldset>
    </div>
</div>
<!-- /.row -->

